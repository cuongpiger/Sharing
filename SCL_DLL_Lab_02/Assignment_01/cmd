hadoop fs -mkdir /Assignment_01

hadoop fs -mkdir /Assignment_01/Input

hadoop fs -put '/home/ubuntu/Desktop/Assignment_01/input.txt' /Assignment_01/Input

cd /home/ubuntu/Desktop/Assignment_01/

javac -classpath ${HADOOP_CLASSPATH} -d '/home/ubuntu/Desktop/Assignment_01/classes' '/home/ubuntu/Desktop/Assignment_01/Assignment_01.java'

jar -cvf Assignment_01.jar -C classes/ .

hadoop jar '/home/ubuntu/Desktop/Assignment_01/Assignment_01.jar' Assignment_01 /Assignment_01/Input /Assignment_01/Output
